<div class="form-group">
  <label for="title">First Name</label>
  <input required value="<%= student.firstName %>" type="text" name="firstName" id="firstName" class="form-control">
</div>
<div class="form-group">
  <label for="title">Last Name</label>
  <input required value="<%= student.lastName %>" type="text" name="lastName" id="lastName" class="form-control">
</div>
<div class="form-group">
  <label for="title">Age</label>
  <input required value="<%= student.age %>" type="text" name="age" id="age" class="form-control">
</div>
<div class="form-group">
  <label for="title">Mobile Number</label>
  <input required value="<%= student.mobileNo %>" type="text" name="mobileNo" id="mobileNo" class="form-control">
</div>
<div class="form-group">
  <label for="title">Email</label>
  <input required value="<%= student.email %>" type="text" name="email" id="email" class="form-control">
</div>
<div class="form-group">
  <label for="title">profilePic</label>
  <input required value="<%= student.profilePic %>" type="text" name="profilePic" id="profilePic" class="form-control">
</div>


<a href="/" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>